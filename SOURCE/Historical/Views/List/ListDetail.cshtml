@*@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}*@
@model RelicsOutputModel

<div>
    <section class="services-section section-padding30 fix">
        <div class="container">
            <h1>Đây là chi tiết</h1>
            <div class="row">
                <div class="col-lg-4 col-md-6 col-sm-6">
                    <div class="single-cat mb-40">
                        <div class="cat-icon">

                            <img src="@Model.ImageUrl" alt="">
                        </div>
                        <div class="cat-cap">
                            <h5><a href="">@Model.RelicsName</a></h5>
                            <p>@Model.Address</p>
                        </div>
                    </div>
                </div>
                <div>
                    <a href="@Model.MapUrl"></a>
                </div>
            </div>
        </div>
    </section>
</div>

